AAA,CAA8F,sBAA9F,EAA8F;AAAjF,IAAA,EAAE,EAAC,IAA8E;AAAzE,IAAA,EAAE,EAAC;AAAsE,GAA9F,E;sBAAiC;AAAA,aAAwC,CAAxC,YAAA,CAAwC,4BAAxC,EAAwC;AAArB,QAAA,IAAI,EAAC;AAAgB,OAAxC,CAAwC,E,WAAA,CAAxC;AAAA,K;;GAAjC,CAAmG,CAAvG,CACI,CAHN,C,iCAYG,EAPQ,QAAA,CAAA,W,kBAAX,mBAAA,CAMM,KANN,EAMM,WANN,EAMM,CALJ,mBAAA,CAAoF,IAApF,EAAoF,IAApF,EAAoF,CAAhF,YAAA,CAA2E,sBAA3E,EAA2E;AAA9D,IAAA,EAAE,EAAC,IAA2D;AAAtD,IAAA,EAAE,EAAC;AAAmD,GAA3E,E;sBAAmC;AAAA,aAA0B,C,kCAAvB,QAAA,CAAA,WAAA,CAAY,Q,GAAQ,C,CAAG,CAA1B;AAAA,K;;GAAnC,CAAgF,CAApF,CAKI,EAJJ,mBAAA,CAGK,IAHL,EAGK,IAHL,EAGK,CAHD,mBAAA,CAEE,GAFF,EAEE;AAFC,IAAA,EAAE,EAAC,IAEJ;AAFU,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA;AAAA,aAAU,QAAA,CAAA,MAAA,IAAA,QAAA,CAAA,MAAA,OAAA,QAAA,YAAV;AAAA,KAAA,EAAgB,CAAA,SAAA,CAAhB,CAAA;AAEf,GAFF,EAAmC,UAAnC,CAGC,CAHL,CAII,CANN,C,iCAOG,CA/BL,CAiCI,CAlCN,CAuCI,EAHJ,mBAAA,CAEM,KAFN,EAAA,WAAA,EAEM,CADJ,YAAA,CAAe,sBAAf,CACI,CAFN,CAGI,CAxCN,C","sourcesContent":["<template>\n  <div id=\"app\">\n    <div class=\"navb\">\n      <ul>\n        <div>\n          <li><router-link to=\"/home\" id=\"nl\"><font-awesome-icon icon=\"home\" /></router-link></li>\n          <li><router-link v-if=\"currentUser\" to=\"/posts\" id=\"nl\">Posts</router-link></li>\n          <li><router-link v-if=\"currentUser\" to=\"/add\" id=\"nl\">Add</router-link></li>\n          <!-- <li><router-link to=\"/home\" id=\"nl\">Home</router-link></li> -->\n          <!-- <li><a  id=\"nl\" href=\"/\">Blog</a></li> -->\n          <!-- <li><router-link id=\"nl\" to=\"/home\">Home</router-link></li> -->\n          <li v-if=\"showAdminBoard\">\n            <router-link id=\"nl\" to=\"/admin\">Admin Board</router-link>\n          </li>\n          <li v-if=\"showModeratorBoard\">\n            <router-link id=\"nl\" to=\"/mod\">Moderator Board</router-link>\n          </li>\n          <li>\n            <router-link id=\"nl\" v-if=\"currentUser\" to=\"/user\">User Content</router-link>\n          </li>\n        </div>\n\n        <div v-if=\"!currentUser\">\n          <li><router-link id=\"nl\" to=\"/register\"><font-awesome-icon icon=\"user-plus\" />  Sign Up</router-link></li>\n          <li><router-link id=\"nl\" to=\"/login\"><font-awesome-icon icon=\"sign-in-alt\" />  Login</router-link></li>\n        </div>\n\n        <div v-if=\"currentUser\">\n          <li><router-link id=\"nl\" to=\"/profile\">{{ currentUser.username }}</router-link></li>\n          <li><a id=\"nl\" @click.prevent=\"logOut\">\n              LogOut\n            </a>\n          </li>\n        </div>\n      </ul>\n      \n    </div>\n\n    <div class=\"container\">\n      <router-view />\n    </div>\n  </div>\n</template>\n\n<script>\nimport EventBus from \"./common/EventBus\";\n\nexport default {\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    },\n    showAdminBoard() {\n      if (this.currentUser && this.currentUser['roles']) {\n        return this.currentUser['roles'].includes('ROLE_ADMIN');\n      }\n\n      return false;\n    },\n    showModeratorBoard() {\n      if (this.currentUser && this.currentUser['roles']) {\n        return this.currentUser['roles'].includes('ROLE_MODERATOR');\n      }\n\n      return false;\n    }\n  },\n  methods: {\n    logOut() {\n      this.$store.dispatch('auth/logout');\n      this.$router.push('/login');\n    }\n  },\n  mounted() {\n    EventBus.on(\"logout\", () => {\n      this.logOut();\n    });\n  },\n  beforeUnmount() {\n    EventBus.remove(\"logout\");\n  }\n};\n</script>\n\n<style>\n.navb {\n  justify-content: space-between;\n  align-items: center;\n  \n}\nul {\n  list-style-type: none;\n  margin: 0;\n  padding: 0;\n  overflow: hidden;\n  background-color: #f7f7f7;\n  top: 0;\n}\n\nli {\n  display: inline;\n  float: left;\n  border-right: 1px solid #bbb;\n}\n\nli #nl {\n  display: block;\n  color:#ADADAD;\n  text-align: center;\n  padding: 14px 16px;\n  text-decoration: none;\n}\n\n/* Change the link color to #111 (black) on hover */\nli #nl:hover {\n  color: black;\n}\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 0px;\n}\n</style>\n"],"sourceRoot":""}]}